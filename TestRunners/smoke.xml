<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Frigidaire Automation Suite" parallel="methods" thread-count="4">
    <test name="Frigidaire Smoke Tests">
        <parameter name="browser" value="chrome"/>
        <groups>
            <run>
                <include name="smoke"/>
            </run>
        </groups>
        <packages>
            <package name="com.automation.frigidaire"/>
            <package name="com.automation.electrolux"/>
        </packages>
    </test>
</suite>